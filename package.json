{
  "author": "Rahul Roy",
  "license": "MIT",
  "name": "cucumber-boilerplate-for-api",
  "version": "2.0.0",
  "description": "Boilerplate project to run WebdriverIO tests with Cucumber for api tests",
  "type": "module",
  "scripts": {
    "test": "run-s test:*",
    "test:lint": "eslint 'src/**/*.ts' 'test/**/*.js!(setup.js)'",
    "test:unit": "jest --config=jest.config.cjs --detectOpenHandles",
    "test:features": "wdio run wdio.BUILD.conf.ts",
    "wdio": "wdio run wdio.conf.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@wdio/cli": "9.0.7",
    "@wdio/config": "9.0.6",
    "@wdio/cucumber-framework": "9.0.6",
    "@wdio/globals": "9.0.7",
    "@wdio/local-runner": "9.0.7",
    "@wdio/spec-reporter": "9.0.7",
    "@wdio/static-server-service": "9.0.4",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-wdio": "^9.0.5",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "npm-run-all2": "^6.2.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "webdriverio": "9.0.7",
    "supertest" : "7.0.0",
    "@types/supertest": "6.0.2",
    "@wdio/types": "9.1.2",
    "wdio-cucumberjs-json-reporter": "6.0.1",
    "multiple-cucumber-html-reporter": "3.7.0"
  }
}
